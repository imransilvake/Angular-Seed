{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"defaultProject": "seed",
	"projects": {
		"seed": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"prefix": "app",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss",
					"skipTests": true
				},
				"@schematics/angular:module": {
					"skipTests": false
				},
				"@schematics/angular:service": {
					"skipTests": false
				},
				"@schematics/angular:pipe": {
					"skipTests": false
				},
				"@schematics/angular:directive": {
					"skipTests": false
				}
			},
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/seed",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"aot": true,
						"assets": [
							"src/favicon.ico",
							"src/assets/images"
						],
						"styles": [
							"node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
							"src/styles.scss"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"src/assets/scss/"
							]
						},
						"scripts": []
					},
					"configurations": {
						"next": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.next.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true
						},
						"prod": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true
						},
						"prod-en": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"outputPath": "dist/seed-en/",
							"baseHref": "/en/",
							"i18nFile": "src/locale/translation.en-US.xlf",
							"i18nFormat": "xlf",
							"i18nLocale": "en-US",
							"i18nMissingTranslation": "error"
						},
						"prod-de": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"outputPath": "dist/seed-de/",
							"baseHref": "/de/",
							"i18nFile": "src/locale/translation.de.xlf",
							"i18nFormat": "xlf",
							"i18nLocale": "de",
							"i18nMissingTranslation": "error"
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "seed:build"
					},
					"configurations": {
						"stag": {
							"browserTarget": "seed:build:stag"
						},
						"next": {
							"browserTarget": "seed:build:next"
						},
						"prod": {
							"browserTarget": "seed:build:prod"
						},
						"prod-en": {
							"browserTarget": "seed:build:prod-en"
						},
						"prod-de": {
							"browserTarget": "seed:build:prod-de"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "seed:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "src/tsconfig.spec.json",
						"karmaConfig": "src/karma.conf.js",
						"styles": [
							"src/styles.scss"
						],
						"scripts": [],
						"assets": [
							"src/favicon.ico",
							"src/assets"
						]
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"src/tsconfig.app.json",
							"src/tsconfig.spec.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				}
			}
		},
		"seed-e2e": {
			"root": "e2e/",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "e2e/protractor.conf.js",
						"devServerTarget": "seed:serve"
					},
					"configurations": {
						"stag": {
							"devServerTarget": "seed:serve:stag"
						},
						"next": {
							"devServerTarget": "seed:serve:next"
						},
						"prod": {
							"devServerTarget": "seed:serve:prod"
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": "e2e/tsconfig.e2e.json",
						"exclude": [
							"**/node_modules/**"
						]
					}
				}
			}
		}
	}
}
