{
	"name": "app-manager",
	"version": "0.0.1",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --port 1500",
		"build": "ng build",
		"serve.app.next": "ng serve -c next --port 2000",
		"build.app.next": "ng build -c next",
		"serve.app.stag": "ng serve -c stag --port 3000",
		"build.app.stag": "ng build -c stag",
		"serve.app.prod": "ng serve -c prod --port 4000",
		"build.app.prod": "ng build -c prod",
		"serve.app.prod-en": "ng serve -c prod-en --port 4001",
		"build.app.prod-en": "ng build -c prod-en",
		"serve.app.prod-de": "ng serve -c prod-de --port 4002",
		"build.app.prod-de": "ng build -c prod-de",
		"lint:ts": "ng lint",
		"lint:scss": "stylelint \"src/**/*.scss\" --syntax scss",
		"translate": "ng xi18n --output-path locale --out-file translation-temp.xlf",
		"translate:ts": "ngx-extractor --input=src/**/*.ts --format=xlf --out-file=src/locale/translation-temp.xlf",
		"test": "ng test",
		"e2e": "ng e2e"
	},
	"author": "Imran Khan",
	"private": true,
	"dependencies": {
		"@angular/animations": "^7.2.14",
		"@angular/cdk": "^7.3.7",
		"@angular/common": "^7.2.14",
		"@angular/compiler": "^7.2.14",
		"@angular/core": "^7.2.14",
		"@angular/forms": "^7.2.14",
		"@angular/http": "^7.2.14",
		"@angular/material": "^7.3.7",
		"@angular/platform-browser": "^7.2.14",
		"@angular/platform-browser-dynamic": "^7.2.14",
		"@angular/router": "^7.2.14",
		"@fortawesome/angular-fontawesome": "^0.3.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.17",
		"@fortawesome/free-solid-svg-icons": "^5.8.1",
		"@imransilvake/scss-framework": "^2.0.9",
		"@ngrx/core": "^1.2.0",
		"@ngrx/store": "^7.4.0",
		"@ngrx/store-devtools": "^7.4.0",
		"@ngx-translate/i18n-polyfill": "^1.0.0",
		"core-js": "^3.0.1",
		"hammerjs": "^2.0.8",
		"moment": "^2.24.0",
		"npm-check-updates": "^3.1.8",
		"rxjs": "^6.5.1",
		"yarn": "^1.15.2",
		"zone.js": "^0.9.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.13.8",
		"@angular/cli": "~7.3.8",
		"@angular/compiler-cli": "^7.2.14",
		"@angular/language-service": "^7.2.14",
		"@types/jasmine": "^3.3.12",
		"@types/jasminewd2": "^2.0.6",
		"@types/node": "~11.13.8",
		"codelyzer": "~5.0.1",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "^4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "^2.0.5",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"protractor": "^5.4.2",
		"stylelint": "^10.0.1",
		"stylelint-order": "^3.0.0",
		"ts-node": "~8.1.0",
		"tslint": "~5.16.0",
		"typescript": "~3.4.5"
	}
}
