{
    "name": "app-manager",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --host 192.168.0.171 --port 1500",
        "build": "ng build",
        "serve.app.next": "ng serve -c next --port 2000",
        "build.app.next": "ng build -c next",
        "serve.app.stag": "ng serve -c stag --port 3000",
        "build.app.stag": "ng build -c stag",
        "serve.app.prod": "ng serve -c prod --port 4000",
        "build.app.prod": "ng build -c prod",
        "serve.app.prod-en": "ng serve -c prod-en --port 4001",
        "build.app.prod-en": "ng build -c prod-en",
        "serve.app.prod-de": "ng serve -c prod-de --port 4002",
        "build.app.prod-de": "ng build -c prod-de",
        "lint:ts": "ng lint",
        "lint:scss": "stylelint \"src/**/*.scss\" --syntax scss",
        "translate": "ng xi18n --output-path locale --out-file translation-temp.xlf",
        "translate:ts": "ngx-extractor --input=src/**/*.ts --format=xlf --out-file=src/locale/translation-temp.xlf",
        "test": "ng test",
        "e2e": "ng e2e"
    },
    "author": "Imran Khan",
    "private": true,
    "dependencies": {
        "@angular/animations": "^8.0.0",
        "@angular/cdk": "^8.0.0",
        "@angular/common": "^8.0.0",
        "@angular/compiler": "^8.0.0",
        "@angular/core": "^8.0.0",
        "@angular/forms": "^8.0.0",
        "@angular/material": "^8.0.0",
        "@angular/platform-browser": "^8.0.0",
        "@angular/platform-browser-dynamic": "^8.0.0",
        "@angular/router": "^8.0.0",
        "@fortawesome/angular-fontawesome": "^0.4.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.18",
        "@fortawesome/free-solid-svg-icons": "^5.8.2",
        "@imransilvake/scss-framework": "^2.1.0",
        "@ngrx/core": "^1.2.0",
        "@ngrx/store": "^8.0.0",
        "@ngrx/store-devtools": "^8.0.0",
        "@ngx-translate/i18n-polyfill": "^1.0.0",
        "core-js": "^3.1.2",
        "crypto-js": "^3.1.9-1",
        "hammerjs": "^2.0.8",
        "jwt-decode": "^2.2.0",
        "moment": "^2.24.0",
        "rxjs": "^6.5.2",
        "zone.js": "~0.9.1",
        "zxcvbn": "^4.4.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.800.0",
        "@angular/cli": "~8.0.2",
        "@angular/compiler-cli": "^8.0.0",
        "@angular/language-service": "^8.0.0",
        "@types/jasmine": "^3.3.12",
        "@types/jasminewd2": "^2.0.6",
        "@types/node": "~12.0.2",
        "codelyzer": "^5.0.1",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^2.0.5",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "protractor": "^5.4.2",
        "stylelint": "^10.0.1",
        "stylelint-order": "^3.0.0",
        "ts-node": "~8.2.0",
        "tslint": "~5.17.0",
        "typescript": "~3.4.5"
    }
