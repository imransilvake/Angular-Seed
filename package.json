{
    "name": "angular-seed",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --port 1500",
        "build": "ng build",
        "serve.app.next": "ng serve -c next --port 2000",
        "build.app.next": "ng build -c next",
        "serve.app.prod": "ng serve -c prod --port 3000",
        "build.app.prod": "ng build -c prod",
        "serve.app.prod-en": "ng serve -c prod-en --port 3001",
        "build.app.prod-en": "ng build -c prod-en",
        "serve.app.prod-de": "ng serve -c prod-de --port 3002",
        "build.app.prod-de": "ng build -c prod-de",
        "lint:ts": "ng lint",
        "lint:scss": "stylelint \"src/**/*.scss\" --syntax scss",
        "translate": "ng xi18n --output-path locale --out-file translation-temp.xlf",
        "translate:ts": "ngx-extractor --input=src/**/*.ts --format=xlf --out-file=src/locale/translation-temp.xlf",
        "test:run": "cypress run",
        "test:open": "cypress open",
        "e2e": "ng e2e"
    },
    "author": "Imran Khan",
    "private": true,
    "dependencies": {
        "@angular/animations": "~10.0.4",
        "@angular/cdk": "^10.0.2",
        "@angular/common": "~10.0.4",
        "@angular/compiler": "~10.0.4",
        "@angular/core": "~10.0.4",
        "@angular/forms": "~10.0.4",
        "@angular/localize": "^10.0.4",
        "@angular/material": "^10.0.2",
        "@angular/platform-browser": "~10.0.4",
        "@angular/platform-browser-dynamic": "~10.0.4",
        "@angular/router": "~10.0.4",
        "@fortawesome/angular-fontawesome": "^0.7.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.30",
        "@fortawesome/free-brands-svg-icons": "^5.14.0",
        "@fortawesome/free-regular-svg-icons": "^5.14.0",
        "@fortawesome/free-solid-svg-icons": "^5.14.0",
        "@imransilvake/scss-framework": "^2.3.2",
        "@ngx-translate/i18n-polyfill": "^1.0.0",
        "hammerjs": "^2.0.8",
        "moment": "^2.27.0",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1000.3",
        "@angular/cli": "~10.0.3",
        "@angular/compiler-cli": "~10.0.4",
        "@angular/language-service": "^10.0.4",
        "@types/node": "^14.0.23",
        "codelyzer": "^6.0.0",
        "ts-node": "~8.10.2",
        "tslint": "~6.1.2",
        "typescript": "~3.9.7",
		"cypress": "^4.10.0"
    }
}
